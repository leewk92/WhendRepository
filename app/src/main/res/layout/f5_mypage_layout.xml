<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/user_photo"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:src="@drawable/userimage_default"
                />
            <LinearLayout
                android:id="@+id/user_clickableLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation = "vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="3dp">
                    <TextView
                        android:id="@+id/username"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="12dp"
                        android:textColor="#212121"
                        android:gravity="center"
                        android:layout_marginLeft="10dp"
                        />

                    <TextView
                        android:id="@+id/realname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="12dp"
                        android:textColor="#b6b6b6"
                        android:gravity="center"
                        android:layout_marginLeft="10dp"
                        />

                </LinearLayout>

                <TextView
                    android:id="@+id/status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="12dp"
                    android:textColor="#b6b6b6"
                    android:gravity="center"
                    android:layout_marginLeft="10dp"
                    />

            </LinearLayout>
        </LinearLayout>
<!--
        <ImageView
            android:id="@+id/follow_button"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/like_on"
            android:layout_alignParentRight="true"
            android:layout_marginTop="7.5dp" />-->
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop = "5dp">

        <LinearLayout
            android:id= "@+id/schedule_count_clickablelayout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="vertical"
            android:layout_weight="1">
            <TextView
                android:id= "@+id/schedule_count"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text=""
                android:textSize="20dp"
                android:textColor="#03A9F4"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="게시물"
                android:textSize="15dp"
                android:textColor="#727272"
                android:gravity="center"/>

        </LinearLayout>
        <LinearLayout
            android:id= "@+id/follower_count_clickablelayout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="vertical"
            android:layout_weight="1">

            <TextView
                android:id= "@+id/follower_count"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text=""
                android:textSize="20dp"
                android:textColor="#03A9F4"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="팔로워"
                android:textSize="15dp"
                android:textColor="#727272"
                android:gravity="center"/>

        </LinearLayout>
        <LinearLayout
            android:id= "@+id/following_count_clickablelayout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="vertical"
            android:layout_weight="1">
            <TextView
                android:id= "@+id/following_count"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text=""
                android:textSize="20dp"
                android:textColor="#03A9F4"
                android:gravity="center"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="팔로잉"
                android:textSize="15dp"
                android:textColor="#727272"
                android:gravity="center"/>

        </LinearLayout>

    </LinearLayout>

    <android.support.v4.app.FragmentTabHost

        android:id="@android:id/tabhost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp">
            <FrameLayout

                android:id="@+id/realtabcontent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />
            </LinearLayout>
    </android.support.v4.app.FragmentTabHost>
</LinearLayout>
