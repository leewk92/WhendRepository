<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="net.whend.soodal.whend.view.F3_Upload"
    android:id="@+id/search_rootlayout">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/search_grid">

        <net.whend.soodal.whend.util.observablescrollview.ObservableScrollView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/quilt_scroll" >

            <net.whend.soodal.whend.util.quitview.QuiltView
                android:id="@+id/quilt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="5dip"
                app:scrollOrientation="vertical">

            </net.whend.soodal.whend.util.quitview.QuiltView>
        </net.whend.soodal.whend.util.observablescrollview.ObservableScrollView>

    </LinearLayout>

    <FrameLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/search_linear"
        >
    </FrameLayout>

</FrameLayout>
